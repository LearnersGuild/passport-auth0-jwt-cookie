{
  "name": "@learnersguild/passport-auth0-jwt-cookie",
  "version": "0.1.2",
  "description": "LearnersGuild authentication module for Auth0 in Node.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "babel -d lib/ src/",
    "prepublish": "npm test && npm run build",
    "test": "./node_modules/.bin/xo"
  },
  "keywords": [
    "auth0"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LearnersGuild/lg-auth0-node-jwt-cookie.git"
  },
  "author": {
    "name": "Jeffrey Wescott",
    "email": "jeffrey@learnersguild.org"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "express-jwt": "^3.3.0",
    "isomorphic-fetch": "^2.2.1",
    "passport": "^0.3.2",
    "passport-auth0": "^0.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0"
  },
  "xo": {
    "ignore": [
      "lib/**"
    ],
    "rules": {
      "comma-dangle": [
        0
      ],
      "no-negated-condition": [
        0
      ],
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ]
    },
    "esnext": true,
    "space": true,
    "semicolon": false
  }
}
